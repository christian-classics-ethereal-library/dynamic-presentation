{
  "name": "dynamic-presentation",
  "version": "2.0.0",
  "description": "Javascript for Dynamic Presentation",
  "main": "js/LoadRevealMusicXMLPlugin.js",
  "scripts": {
    "run": "npx http-server",
    "setupAndServe": "git submodule update --init --recursive && npm install && npm run run",
    "style:check": "npx prettierx --check {**/,}*.{css,html,js,json,md,scss,yml} && npx semistandard js/*.js",
    "style:fix": "npx prettierx --write {**/,}*.{css,html,js,json,md,scss,yml}; npx semistandard js/*.js --fix",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git@gitlab.ccel.org:hymnary/dynamic-presentation.git"
  },
  "author": "Zach DeCook <zdecook@ccel.org>",
  "license": "ISC",
  "devDependencies": {
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "http-server": "^0.11.1",
    "jest": "^24.9.0",
    "jest-environment-jsdom-fourteen": "^0.1.0",
    "prettierx": "^0.11.0",
    "semistandard": "^13.0.1"
  },
  "jest": {
    "collectCoverageFrom": [
      "js/*.js"
    ],
    "coverageDirectory": "coverage",
    "testEnvironment": "jest-environment-jsdom-fourteen"
  },
  "dependencies": {
    "impress.js": "^1.0.0"
  }
}
